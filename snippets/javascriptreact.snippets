snippet reducer "Reducer boilerplate"
  const initialState = {
    error: false,
  };

  const actionTypes = {
    error: 'ERROR',
  };

  const reducerObject = (state, payload) => ({
    [actionTypes.error]: {
      ...state,
      error: true,
    },
  });

  const reducer = (state, action) =>
    reducerObject(state, action.payload)[action.type] || state;

  export { reducer, actionTypes, reducerObject, initialState };
